{
  "$schema": "appsettings-schema.json",
  "ConnectionStrings": {
//    "umbracoDbDSN": "Data Source=|DataDirectory|/Umbraco.sqlite.db;Cache=Shared;Foreign Keys=True;Pooling=True",
//    "umbracoDbDSN_ProviderName": "Microsoft.Data.Sqlite"
//    "umbracoDbDSN": "Server=127.0.0.1,55045;User ID=sa;Password=Password1234!;TrustServerCertificate=true;Initial Catalog=umbraco-cms",
//    "umbracoDbDSN_ProviderName": "Microsoft.Data.SqlClient"
    "umbracoDbDSN": "<from-aspire>",
    "umbracoDbDSN_ProviderName": "<from-aspire>"
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "Microsoft.Hosting.Lifetime": "Information",
        "System": "Warning"
      }
    },
    "WriteTo": [
      {
        "Name": "Async",
        "Args": {
          "configure": [
            {
              "Name": "Console"
            }
          ]
        }
      }
    ]
  },
  "Umbraco": {
    "CMS": {
      "Global": {
        "Id": "8c27929c-978f-494f-ae4c-272f53c991bb"
      },
      "Content": {
        "AllowEditInvariantFromNonDefault": true,
        "ContentVersionCleanupPolicy": {
          "EnableCleanup": true
        }
      },
      "Unattended": {
        "UpgradeUnattended": true
      },
      "Security": {
        "AllowConcurrentLogins": false
      },
      "Hosting": {
        "Debug": true
      },
      "ModelsBuilder": {
        "ModelsMode": "SourceCodeAuto",
        "IncludeVersionNumberInGeneratedModels": false,
        "FlagOutOfDateModels": true
      },
      "WebRouting": {
        "DisableRedirectUrlTracking": true
      }
    }
  },
  "uSync": {
    "Settings": {
      "AddOnPing": false,
      "ImportAtStartup": "All",
      "ImportOnFirstBoot": true
    }
  }
}